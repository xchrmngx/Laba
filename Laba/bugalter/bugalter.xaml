        <Image HorizontalAlignment="Left" Height="135" Margin="674,10,0,0" VerticalAlignment="Top" Width="89" Source="../img/Бухгалтер.png"/>
        <Label Name="FIO" Content="Названия" HorizontalAlignment="Left" Margin="530,10,0,0" VerticalAlignment="Top" Height="27" Width="139"/>
        <Label Content="Бухгалтер" HorizontalAlignment="Left" Margin="674,148,0,0" VerticalAlignment="Top" Width="84" HorizontalContentAlignment="Center"/>
        <Image HorizontalAlignment="Left" Height="109" Margin="10,10,0,0" VerticalAlignment="Top" Width="123" Source="/Laba;component/img/logo.png"/>
    </Grid>
</Window>
